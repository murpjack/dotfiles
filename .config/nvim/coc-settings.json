{
  "coc.preferences.formatOnSaveFiletypes": [
    "elm",
    "css",
    "scss",
    "javascript",
    "typescript"
  ],
 "prettier.tabWidth": 2,
 "coc.preferences.codeLens.enable": true,
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"]
    }
  }
}
